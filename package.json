{
  "name": "mpe-merch-workbench-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "ng": "nx",
    "nx": "nx",
    "start": "npm run serve:local",
    "start:full": "run-s format:fix lint:all test:all serve:local",
    "serve:local": "nx serve --c=local",
    "serve:qa": "nx serve --c=qa",
    "serve:dev": "nx serve --c=dev",
    "serve:prod": "nx serve --c=production",
    "build": "ng build",
    "test": "ng test",
    "test-ci": "nx test --karma-config=karma-ci.conf.js --no-progress",
    "test:all": "nx run-many --projects=home,shared,sgm,assortment-management,auth,asmt-mgmt-service --target=test --karma-config=karma-ci.conf.js --no-progress",
    "test:failed": "nx run-many --all --target=test --only-failed",
    "lint": "nx workspace-lint && ng lint",
    "lint:all": "nx run-many --projects=home,shared,sgm,assortment-management,auth,asmt-mgmt-service --target=lint",
    "lint:failed": "nx run-many --all --target=lint --only-failed",
    "e2e": "ng e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "ng update @nrwl/workspace",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "format:fix": "pretty-quick --staged",
    "precommit": "run-s format:fix lint",
    "prepush": "run-s test"
  },
  "private": true,
  "dependencies": {
    "@ag-grid-community/all-modules": "^23.2.1",
    "@angular/animations": "10.0.0",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "10.0.0",
    "@angular/compiler": "10.0.0",
    "@angular/core": "10.0.0",
    "@angular/forms": "10.0.0",
    "@angular/material": "^9.2.4",
    "@angular/platform-browser": "10.0.0",
    "@angular/platform-browser-dynamic": "10.0.0",
    "@angular/router": "10.0.0",
    "@azure/msal-angular": "^1.1.2",
    "@azure/msal-browser": "^2.5.2",
    "@ngrx/effects": "10.0.0",
    "@ngrx/entity": "10.0.0",
    "@ngrx/router-store": "10.0.0",
    "@ngrx/store": "10.0.0",
    "@nrwl/angular": "10.1.0",
    "ag-grid-angular": "^23.2.1",
    "ag-grid-community": "^23.2.1",
    "ag-grid-enterprise": "^23.2.1",
    "bootstrap": "^4.5.2",
    "hammerjs": "^2.0.8",
    "msal": "^1.4.5",
    "rxjs": "6.5.5",
    "zone.js": "^0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "0.1000.8",
    "@angular/cli": "10.0.8",
    "@angular/compiler-cli": "10.0.0",
    "@angular/language-service": "10.0.0",
    "@ngrx/schematics": "10.0.0",
    "@ngrx/store-devtools": "10.0.0",
    "@nrwl/cypress": "10.1.0",
    "@nrwl/jest": "10.1.0",
    "@nrwl/workspace": "^10.3.1",
    "@types/jasmine": "~2.8.8",
    "@types/jest": "26.0.8",
    "@types/node": "~8.9.4",
    "codelyzer": "~5.0.1",
    "cypress": "^4.12.1",
    "dotenv": "6.2.0",
    "eslint": "6.8.0",
    "husky": "^4.2.5",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "jest": "26.2.2",
    "jest-preset-angular": "8.2.1",
    "karma": "^5.2.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.0.4",
    "pretty-quick": "^2.0.2",
    "ts-jest": "26.1.4",
    "ts-node": "~7.0.0",
    "tslint": "~6.1.0",
    "typescript": "3.9.7"
  }
}
