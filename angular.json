{
  "version": 1,
  "projects": {
    "home": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/home",
      "sourceRoot": "apps/home/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/home",
            "index": "apps/home/src/index.html",
            "main": "apps/home/src/main.ts",
            "polyfills": "apps/home/src/polyfills.ts",
            "tsConfig": "apps/home/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/home/src/favicon.ico",
              "apps/home/src/assets"
            ],
            "styles": [
              "apps/home/src/styles.scss"
            ],
            "scripts": [],
            "preserveSymlinks": true,
            "allowedCommonJsDependencies": [
              "ag-grid-enterprise",
              "hammerjs"
            ]
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/home/src/environments/environment.ts",
                  "with": "apps/home/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "local": {
              "fileReplacements": [
                {
                  "replace": "apps/home/src/environments/environment.ts",
                  "with": "apps/home/src/environments/environment.local.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "qa": {
              "fileReplacements": [
                {
                  "replace": "apps/home/src/environments/environment.ts",
                  "with": "apps/home/src/environments/environment.qa.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            },
            "dev": {
              "fileReplacements": [
                {
                  "replace": "apps/home/src/environments/environment.ts",
                  "with": "apps/home/src/environments/environment.dev.ts"
                }
              ],
              "optimization": false,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": false
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "home:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "home:build:production"
            },
            "local": {
              "browserTarget": "home:build:local"
            },
            "qa": {
              "browserTarget": "home:build:qa"
            },
            "dev": {
              "browserTarget": "home:build:dev"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "home:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/home/tsconfig.app.json",
              "apps/home/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/home/**/*"
            ]
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/home/src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "apps/home/src/polyfills.ts",
            "tsConfig": "apps/home/tsconfig.spec.json",
            "scripts": [],
            "styles": [
              "apps/home/src/assets/scss/material-dashboard.scss"
            ],
            "assets": [
              "apps/home/src/assets",
              "apps/home/src/favicon.ico"
            ]
          }
        }
      }
    },
    "home-e2e": {
      "root": "apps/home-e2e",
      "sourceRoot": "apps/home-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/home-e2e/cypress.json",
            "tsConfig": "apps/home-e2e/tsconfig.e2e.json",
            "devServerTarget": "home:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "home:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/home-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/home-e2e/**/*"
            ]
          }
        }
      }
    },
